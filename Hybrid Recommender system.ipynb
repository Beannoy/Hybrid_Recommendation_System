        "id": "78424c8a"
      },
      "outputs": [],
      "source": [
        "user_ratings_mean = np.mean(RM, axis = 1)\n",
        "\n",
        "RM_demeaned = RM - user_ratings_mean.reshape(-1, 1)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "82495bc4",
      "metadata": {
        "id": "82495bc4"
      },
      "outputs": [],
      "source": [
        "from scipy.sparse.linalg import svds\n",
        "# 50 latent factors\n",
        "U, sigma, Vt = svds(RM_demeaned, k = 50)\n",
        "\n",
        "# SVDS on skewed data\n",
        "U_sk, sigma_sk, Vt_sk = svds(RM, k = 50)\n",
        "sigma_sk = np.diag(sigma_sk)"
      ]
    },
    {
