      "source": [
        "top_10 =pd.DataFrame( columns=[\"user_id\",\"rmse\",\"mae\",\"precision\",\"recall\",\"f1\"])\n",
        "top_20 =pd.DataFrame( columns=[\"user_id\",\"rmse\",\"mae\",\"precision\",\"recall\",\"f1\"])"
      ]
    },
    {
      "cell_type": "code",
